          "{",
          "    #[$(_p = \"$(_tprjs.Split(';')[$(i)].Trim())\")]",
          "    #[try ",
          "    {",
          "        #[( $(_p) != \"\" ) ",
          "        {",
          "            #[File write(\"$(incModules)/$(_p)/.props.user\"): #[$(tplProps.Replace('$v', '#[File get(\"$(_p)/.version\")]'))]]",
          "        }]",
          "    }",
          "    catch(err, msg){ #[IO writeLine(STDERR): Cfg .props.user failed: #[$(msg)] ] }]",
          "}",
          "catch { $(loop = false) }] ]"
        ]
